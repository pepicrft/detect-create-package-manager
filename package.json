{
  "name": "detect-create-package-manager",
  "version": "0.2.0",
  "description": "A utility that returns the package manager used to a create workflow",
  "type": "module",
  "scripts": {
    "prepack": "pnpm build",
    "build": "rimraf dist && tsc",
    "test": "vitest run"
